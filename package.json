{
    "name": "flowlint-github-app",
    "version": "1.6.5",
    "private": true,
    "license": "MIT",
    "type": "commonjs",
    "scripts": {
        "dev:api": "ts-node --project tsconfig.json apps/api/src/server.ts",
        "dev:worker": "ts-node --project tsconfig.json apps/worker/src/worker.ts",
        "build": "tsc -p tsconfig.json",
        "check": "tsc -p tsconfig.json --noEmit",
        "test": "vitest run",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@octokit/app": "^16.0.0",
        "@octokit/auth-app": "^8.0.0",
        "@octokit/plugin-paginate-rest": "^14.0.0",
        "@octokit/webhooks": "^14.0.0",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/auto-instrumentations-node": "^0.67.0",
        "@opentelemetry/exporter-trace-otlp-grpc": "^0.208.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
        "@opentelemetry/instrumentation-express": "^0.57.0",
        "@opentelemetry/instrumentation-http": "^0.208.0",
        "@opentelemetry/resources": "^2.2.0",
        "@opentelemetry/sdk-node": "^0.208.0",
        "@opentelemetry/semantic-conventions": "^1.38.0",
        "@replikanti/flowlint-core": "^0.9.0",
        "bullmq": "^5.0.0",
        "dotenv": "^17.0.0",
        "express": "^4.22.1",
        "express-rate-limit": "^8.2.1",
        "octokit": "^5.0.0",
        "pino": "^10.1.0",
        "pino-pretty": "^13.1.2",
        "prom-client": "^15.1.3",
        "swagger-ui-express": "^5.0.1",
        "yaml": "^2.4.0"
    },
    "devDependencies": {
        "@octokit/rest": "22.0.1",
        "@types/express": "4.17.25",
        "@types/micromatch": "4.0.10",
        "@types/node": "24.10.4",
        "@types/supertest": "^6.0.3",
        "@types/swagger-ui-express": "4.1.8",
        "@vitest/coverage-v8": "^4.0.16",
        "openai": "6.15.0",
        "supertest": "^7.1.4",
        "ts-node": "10.9.2",
        "typescript": "5.9.3",
        "vitest": "4.0.16"
    },
    "engines": {
        "node": ">=22"
    }
}
